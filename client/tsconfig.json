{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es2015",
    "sourceMap": true,
    "importHelpers": true,
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "build",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "lib": [
      
    ],
    "declaration": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@nrwl/detox": [
        "packages/detox"
      ],
      "@nrwl/react-native": [
        "packages/react-native"
      ]
    },
    "jsx": "react-native"
  },
  "exclude": [
    "node_modules",
    "tmp"
  ],
  "extends": "expo/tsconfig.base"
}
